                                           32 + 64, 33 + 64, 34 + 64, 35 + 64, 36 + 64, 37 + 64, 38 + 64, 39 + 64, 40 + 64, 41 + 64, 42 + 64, 43 + 64, 44 + 64, 45 + 64, 46 + 64, 47 + 64, 48 + 64, 49 + 64, 50 + 64, 51 + 64, 52 + 64, 53 + 64, 54 + 64, 55 + 64, 56 + 64, 57 + 64, 58 + 64, 59 + 64, 60 + 64, 61 + 64, 62 + 64, 63 + 64}; \
